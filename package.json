{
  "name": "@stylelint-types/stylelint-stylus",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@10.8.1",
  "description": "TypeScript definitions for stylelint-define-config",
  "author": "pengzhanbo <volodymyr@foxmail.com> (https://github.com/pengzhanbo)",
  "license": "MIT",
  "homepage": "https://github.com/stylelint-types/stylelint-stylus#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/stylelint-types/stylelint-stylus.git"
  },
  "bugs": "https://github.com/stylelint-types/stylelint-stylus/issues",
  "keywords": [
    "config",
    "scss",
    "configuration",
    "define-config",
    "stylelint-config",
    "stylelint",
    "stylelint-stylus",
    "stylelintconfig",
    "typed",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./types": {
      "types": "./lib/types.d.ts"
    }
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "tsup",
    "prepublish": "pnpm lint && pnpm build",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp package.json --execute=\"pnpm release:changelog\" --commit --all --push --tag && pnpm publish --access public"
  },
  "peerDependencies": {
    "stylelint": ">=16.0.0",
    "stylelint-define-config": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "stylelint": {
      "optional": true
    }
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^1.29.0",
    "bumpp": "^10.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.25.0",
    "stylelint": "^16.18.0",
    "stylelint-define-config": "^16.18.0",
    "stylelint-stylus": "^1.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
